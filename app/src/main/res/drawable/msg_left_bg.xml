<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:left="10dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/grey8" />
            <corners android:radius="5dp" />
        </shape>
    </item>

    <item>
        <rotate
            android:fromDegrees="-35"
            android:pivotX="0%"
            android:pivotY="0%"
            android:toDegrees="0"


            >
            <shape android:shape="rectangle">
                <padding
                    android:bottom="5dp"
                    android:left="15dp"
                    android:right="15dp"
                    android:top="5dp" />
                <solid android:color="@color/grey8" />

            </shape>
        </rotate>

    </item>


</layer-list>
